{
  "name": "starwarsdeckbuilding",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:ts": "rollup -c",
    "build:scss": "sass --no-source-map src/game.scss starwarsdeckbuilding.css",
    "watch:ts": "rollup -c -w",
    "watch:scss": "sass --watch src/game.scss starwarsdeckbuilding.css",
    "watch": "npm run watch:ts && npm run watch:scss",
    "build": "npm run build:ts && npm run build:scss"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.3.0",
    "rollup": "^4.53.5",
    "sass": "^1",
    "tslib": "^2.8.1",
    "typescript": "^5"
  }
}
